    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Admin GreenRoad</h1>
    <p class="lead">
        Green Road é a rede social que proporciona a união e a disceminação de informação para aqueles que procuram ou já cultivam hortas em casa
        Além de dispnibilizar um comercio entre os cultivos!
    </p>
    <p><a title="Em Desenvolvimento" class="btn btn-primary btn-lg">Acesse! &raquo;</a></p>
</div>

